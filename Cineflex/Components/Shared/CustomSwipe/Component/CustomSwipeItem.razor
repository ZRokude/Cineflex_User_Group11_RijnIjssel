<MudStack Style="@_style" Justify="Justify.Center" AlignItems="AlignItems.Center">
    <img src="@Image" style="@_imageStyle"/>
    <MudPaper Style="@(_titleStyle)" Elevation="0" MinHeight="20%" MaxHeight="20%">
        <MudText>
            @Title
        </MudText>
    </MudPaper>
</MudStack>
   



