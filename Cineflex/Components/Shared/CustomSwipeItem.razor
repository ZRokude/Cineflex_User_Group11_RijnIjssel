<MudStack Justify="Justify.Center" AlignItems="AlignItems.Center">
    <MudImage Src="@Image" Style="@(_imageStyle)" Fluid="true" ObjectPosition="ObjectPosition.Center" ObjectFit="ObjectFit.Contain">

    </MudImage>
    <MudPaper Style="@(_titleStyle)" Elevation="0" MinHeight="20%" MaxHeight="20%">
        <MudText>
            @Title
        </MudText>
    </MudPaper>
</MudStack>


