@inherits LayoutComponentBase
<MudThemeProvider />
<MudPopoverProvider />
<MudDialogProvider />
<MudSnackbarProvider />

<MudLayout>
    <NavBar/>
    <MudStack Row="true">
        <div class="block-color"/>
        <MudMainContent>
            @Body
        </MudMainContent>
        <div class="block-color"/>
